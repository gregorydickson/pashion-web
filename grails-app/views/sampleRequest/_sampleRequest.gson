import pashion.SampleRequest


model {  
    SampleRequest sampleRequest
}

json g.render(sampleRequest,[excludes:['pressHouse'],expand:['searchableItemsStatus','brand','searchableItemsProposed','deliverTo','searchableItems','requestingUser','returnToAddress','shippingOut','shippingReturn','addressDestination']]){
	pressHouse {
		name sampleRequest.pressHouse.name
	}
}