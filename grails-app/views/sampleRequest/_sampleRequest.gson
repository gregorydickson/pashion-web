import pashion.SampleRequest


model {  
    SampleRequest sampleRequest
}

json g.render(sampleRequest,[excludes:['pressHouse'],expand:['searchableItemsStatus','brand','searchableItemsProposed','searchableItems','requestingUser','returnTo','shippingOut','shippingReturn']]){
	pressHouse {
		name sampleRequest.pressHouse.name
	}
}